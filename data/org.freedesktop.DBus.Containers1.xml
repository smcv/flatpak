<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/org/freedesktop/DBus">
  <interface name="org.freedesktop.DBus.Containers1">
    <method name="AddContainerServer">
      <arg direction="in" type="s" name="type"/>
      <arg direction="in" type="s" name="name"/>
      <arg direction="in" type="a{sv}" name="metadata"/>
      <arg direction="in" type="a{sv}" name="named_arguments"/>
      <arg direction="out" type="o" name="instance"/>
      <arg direction="out" type="ay" name="socket_path"/>
      <arg direction="out" type="s" name="address"/>
    </method>
    <method name="StopContainerInstance">
      <arg direction="in" type="o" name="instance"/>
    </method>
    <method name="StopContainerListening">
      <arg direction="in" type="o" name="instance"/>
    </method>
    <method name="GetConnectionContainerInstance">
      <arg direction="in" type="s" name="bus_name"/>
      <arg direction="out" type="o" name="instance"/>
      <arg direction="out" type="s" name="type"/>
      <arg direction="out" type="s" name="name"/>
      <arg direction="out" type="a{sv}" name="metadata"/>
    </method>
    <method name="GetContainerInstanceInfo">
      <arg direction="in" type="o" name="instance"/>
      <arg direction="out" type="s" name="type"/>
      <arg direction="out" type="s" name="name"/>
      <arg direction="out" type="a{sv}" name="metadata"/>
    </method>
    <property name="SupportedArguments" type="as" access="read">
      <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="const"/>
    </property>
    <signal name="ContainerInstanceRemoved">
      <arg type="o" name="path"/>
    </signal>
  </interface>
</node>

